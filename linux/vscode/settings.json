{
    "editor.fontFamily": "'DroidSansMono Nerd Font Book', 'monospace', monospace, 'Droid Sans Fallback'",
    "terminal.integrated.fontFamily": "'MesloLGS NF'",
    "terminal.integrated.shell.linux": "/usr/bin/zsh",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Bear Theme",
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#2b6cae",
        "activityBar.activeBorder": "#2b6cae",
        "statusBar.background": "#2b6cae",
        "terminal.foreground": "#fdf6e3"
    }
    "explorer.confirmDragAndDrop": false
}
